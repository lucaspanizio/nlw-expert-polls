{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    /* Language and Environment */
    "target": "ES2022",
    "lib": [
      "ES2023"
    ],
    /* Modules */
    "module": "node16",
    /* Emit */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    /* Completeness */
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@http/*": [
        "http/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@ws/*": [
        "ws/*"
      ]
    }
  }
}